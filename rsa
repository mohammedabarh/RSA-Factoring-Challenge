#!/usr/bin/python3
from sys import argv
from math import isqrt

def check_prime(value):
    """Check if the input number is prime."""
    if value < 2:
        return False
    for divisor in range(2, isqrt(value) + 1):
        if value % divisor == 0:
            return False
    return True

with open(argv[1]) as file:
    for line in file:
        num = int(line.strip())
        print("{:d}=".format(num), end="")
        if num % 2 == 0:
            print("{}*2".format(num // 2))
            continue
        for odd_divisor in range(3, isqrt(num) + 1, 2):
            if num % odd_divisor == 0 and check_prime(odd_divisor):
                factor = num // odd_divisor
                print("{}*{}".format(factor, odd_divisor))
                break
