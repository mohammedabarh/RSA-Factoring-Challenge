#!/usr/bin/env python3
"""Factor numbers from a given file."""
from sys import argv
from math import isqrt

def read_numbers_from_file(file_path):
    """Read integers from a file and return them as a list."""
    with open(file_path, "r", encoding="utf-8") as file:
        return [int(line.strip()) for line in file]

def find_factors(num):
    """Find and print two factors of the given number."""
    for factor in range(2, isqrt(num) + 1):
        if num % factor == 0:
            print(f"{num}={num // factor}*{factor}")
            return
    # If no factors are found, print the number as a product of 1 and itself
    print(f"{num}=1*{num}")

def factorize_numbers(numbers):
    """Factor each number in the provided list."""
    for number in numbers:
        find_factors(number)

if __name__ == "__main__":
    if len(argv) == 2:
        factorize_numbers(read_numbers_from_file(argv[1]))
