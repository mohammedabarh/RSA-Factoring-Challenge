#!/usr/bin/python3
import sys
from math import sqrt

def factorize(n):
    """Factorize a number into a product of two smaller numbers."""
    for i in range(2, int(sqrt(n)) + 1):
        if n % i == 0:
            return (i, n // i)
    return (n, 1)

def main(filename):
    """Process the file and factorize each number."""
    try:
        with open(filename, 'r') as file:
            for line in file:
                n = int(line.strip())
                p, q = factorize(n)
                print(f"{n}={p}*{q}")
    except FileNotFoundError:
        print(f"Error: File '{filename}' not found.")
        sys.exit(1)
    except ValueError:
        print(f"Error: Invalid number in the file.")
        sys.exit(1)

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        sys.exit(1)
    main(sys.argv[1])
